generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user{
  id Int @id @default(autoincrement())
  name String?
  email String @unique
  password String?
  driver Boolean @default(false)
  user Boolean @default(false)
  subscription_status String?
}

