generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user{
  id Int @id @default(autoincrement())
  name String?
  email String @unique
  password String?
  driver Boolean @default(false)
  user Boolean @default(false)
  subscription_status String?
}

model jobs{
  job_id Int @id @default(autoincrement())
  user_id Int?
  description String?
  load_weight Int?
  pickup_location String?
  dropoff_location String?
  date String?
  status String?
  price Float?
  distance Float?
  time Float?
}

